{
  "name": "prosemirror-unified",
  "version": "0.8.4",
  "description": "ProseMirror Unified integration",
  "keywords": [
    "prosemirror",
    "unified",
    "unist"
  ],
  "homepage": "https://github.com/marekdedic/prosemirror-unified#readme",
  "bugs": {
    "url": "https://github.com/marekdedic/prosemirror-unified/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marekdedic/prosemirror-unified.git"
  },
  "license": "MIT",
  "author": "Marek Dědič",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/prosemirror-unified.d.ts",
        "default": "./dist/prosemirror-unified.js"
      },
      "require": {
        "types": "./dist/prosemirror-unified.d.cts",
        "default": "./dist/prosemirror-unified.cjs"
      }
    }
  },
  "main": "dist/prosemirror-unified.cjs",
  "module": "dist/prosemirror-unified.js",
  "types": "dist/prosemirror-unified.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "vite build && cp dist/prosemirror-unified.d.ts dist/prosemirror-unified.d.cts",
    "clean": "rimraf dist/* types/*",
    "lint": "run-p -c --aggregate-output lint:*",
    "lint:attw": "attw --pack",
    "lint:eslint": "eslint --color",
    "lint:typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "start": "vite build --watch",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/unist": "^3.0.0",
    "prosemirror-commands": "^1.5.1",
    "prosemirror-inputrules": "^1.2.0",
    "prosemirror-keymap": "^1.2.1",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.0",
    "@eslint/js": "^9.9.1",
    "@eslint/json": "^1.0.0",
    "@eslint/markdown": "^7.0.0",
    "@microsoft/api-extractor": "^7.34.9",
    "@types/node": "^25.0.2",
    "@vitest/coverage-v8": "^3.1.4 <3.2",
    "@vitest/eslint-plugin": "^1.6.7",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-package-json": "^0.88.0",
    "eslint-plugin-perfectionist": "^5.0.0",
    "eslint-plugin-prefer-arrow-functions": "^3.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^28.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.1.4 <3.2",
    "vitest-prosemirror": "^0.3.0"
  },
  "peerDependencies": {
    "prosemirror-model": "^1.24.1",
    "prosemirror-state": "^1.4.3"
  }
}
